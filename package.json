    "fastify-cli": "^4.1.1",
    "fastify-jwt": "^4.2.0",
    "fastify-plugin": "^3.0.0",
    "fastify-zod": "^1.0.2",
    "typescript": "^4.7.4",
    "zod": "^3.17.3",
    "zod-to-json-schema": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
