{
  "name": "simword-api",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "src/app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "build": "tsc -p tsconfig.json",
    "start": "node src/app.js",
    "prisma:generate": "prisma generate",
    "preview": "fastify start -l info src/app.ts",
    "dev:fast": "fastify start -w -l info -P src/app.ts",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child src/app.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/jwt": "5.0.0",
    "@fastify/sensible": "^4.1.0",
    "@fastify/swagger": "^7.4.1",
    "@prisma/client": "^3.15.2",
    "bcrypt": "^5.0.1",
    "fastify": "^4.0.0",
    "fastify-bcrypt": "^1.0.0",
    "fastify-cli": "^4.1.1",
    "fastify-plugin": "^3.0.0",
    "fastify-zod": "^1.0.2",
    "typescript": "^4.7.4",
    "zod": "^3.17.3",
    "zod-to-json-schema": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.0.0",
    "prisma": "3.15.2",
    "tap": "^16.1.0",
    "ts-node-dev": "^2.0.0"
  }
}
